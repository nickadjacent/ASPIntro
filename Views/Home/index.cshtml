<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/style.css">
</head>
<body>
    <h1 class="red">Hello World</h1>

    @{
        string[] travelDestinations = new string[]
        {
            "Longyearbyen",
            "Solovetsky Islands",
            "Socotra",
            "Bhutan",
        };
    }

    @foreach (string item in travelDestinations)
    {
            <li>
                <a href="/travel/@item">@item</a>
            </li>
    }

    <form action=""></form>
</body>
</html>